@article{Salmons1993,
abstract = {Examined the impact of students' grade expectations on their evaluations of instructors' performance. 444 undergraduate students (aged 18–22 yrs) supplied demographic information and completed instructor evaluations concerning graduate teaching fellows 3–4 wks into the course and 2nd evaluations on the university's official evaluation form at 13 wks. Results show that Ss expecting to receive F grades significantly lowered their instructors' evaluations. Those Ss expecting A or B grades significantly raised their instructors' evaluations. Findings suggest that instructors receive higher ratings, in part, because they are more lenient in their grade assignment. It is concluded that there is a significant relationship between the grades students expect to receive and their subsequent evaluation of instruction.},
author = {Salmons, Sherry D.},
journal = {Applied H.R.M Research},
number = {2},
pages = {102--114},
title = {{The relationship between students' grades and their evaluation of instructor performance}},
volume = {4},
year = {1993}
}
